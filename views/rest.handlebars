<div class="container text-center">
    <h1 class="mt-4 mb-0">Your Restaurants</h1>
    <hr class="mt-3 mb-5">
</div>
<div class="container mb-4">
    <div class="card-deck">
        {{#each data}}
        <div class="card mb-3" style="min-width: 300px; max-width: 350px">
            <img src="{{this.rest_img}}" height="350" width="300"
                class="card-img-top">
            <div class="card-body">
                <h5 class="card-title">{{this.resturant_name}}

                    <hr>
                    <p class="card-text">{{this.description}}</p>
            </div>
        </div>
        {{/each}}

    </div>

</div>